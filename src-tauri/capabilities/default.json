{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:app:default",
    "clipboard:read-all",
    "clipboard:write-all",
    "clipboard:monitor-all",
    "global-shortcut:default",
    "window-state:default"
  ]
}